{
  "version": 2,
  "builds": [
    {
      "src": "dummy_backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": {
    "name": "react",
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/build"
  }
}
